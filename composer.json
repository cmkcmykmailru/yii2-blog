{
  "name": "grigor/yii2-blog",
  "description": "blog",
  "type": "yii2-extension",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "keywords": [
    "yii2",
    "extension"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "grigor",
      "email": "cmk.cmyk@mail.ru"
    }
  ],
  "require": {
    "php": ">=7.4",
    "yiisoft/yii2": "~2.0.0",
    "la-haute-societe/yii2-save-relations-behavior": "^1.7.2",
    "paulzi/yii2-nested-sets": "^1.0",
    "elisdn/yii2-composite-form": "^1.1",
    "grigor/yii2-project-library": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.19"
  },
  "autoload": {
    "psr-4": {
      "grigor\\blog\\": "src"
    }
  },
  "scripts": {
    "tests": "php ./vendor/bin/phpunit --colors=always tests"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "path",
      "url": "../yii2-project-library"
    }
  ]
}
